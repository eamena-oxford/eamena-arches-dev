-- query
select resourceinstanceid, tiledata from tiles 
--select count(*) from tiles 
where tiledata -> 'c712066a-8094-11ea-a6a6-02e7594ce0a0' -> 0 ->> 'url' ilike '%staticflick%'
order by resourceinstanceid desc
limit 10



